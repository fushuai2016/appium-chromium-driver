{
  "name": "@rpai/appium-chromium-driver",
  "version": "1.0.1",
  "description": "Appium 2.x driver for Chromium-based browsers that work with Chromedriver",
  "keywords": [
    "appium",
    "automation",
    "javascript",
    "selenium",
    "webdriver",
    "chrome",
    "chromium",
    "google"
  ],
  "homepage": "https://appium.io",
  "bugs": {
    "url": "https://github.com/fushuai2016/appium-chromium-driver/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fushuai2016/appium-chromium-driver"
  },
  "license": "Apache-2.0",
  "author": "https://github.com/appium",
  "directories": {
    "lib": "lib"
  },
  "main": "index.js",
  "files": [
    "lib",
    "config",
    "build",
    "index.js",
    "install-npm.js",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "npm-shrinkwrap.json"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "npm run build -- --watch",
    "clean": "npx rimraf build && npx rimraf node_modules/.cache",
    "reinstall": "npm run clean && npx rimraf node_modules && npm install",
    "update": "npx rimraf package-lock.json && npm run reinstall",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "npm run build",
    "postinstall": "node install-npm.js",
    "test": "npm run test:unit",
    "test:unit": "mocha \"./test/unit/**/*.spec.js\"",
    "test:e2e": "mocha \"./test/e2e/**/*.spec.js\"",
    "test:smoke": "node ./index.js",
    "test:ci": "npm run test:unit && npm run test:e2e && npm run test:smoke"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "singleQuote": true
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "lodash": "4.17.21",
    "@appium/base-driver": "^9.1.0",
    "@appium/support": "^5.1.1",
    "@testim/chrome-version": "1.1.4",
    "@xmldom/xmldom": "^0.x",
    "appium-adb": "^12.0.0",
    "asyncbox": "^3.0.0",
    "axios": "^1.6.5",
    "compare-versions": "^6.0.0",
    "fancy-log": "^2.0.0",
    "semver": "^7.0.0",
    "source-map-support": "^0.x",
    "teen_process": "^2.2.0",
    "xpath": "^0.x"
  },
  "peerDependencies": {
    "appium": "^2.4.0"
  },
  "engines": {
    "node": "^14.17.0 || ^16.13.0 || >=18.0.0",
    "npm": ">=8"
  },
  "publishConfig": {
    "access": "public"
  },
  "access": "public",
  "appium": {
    "driverName": "chromium",
    "automationName": "Chromium",
    "platformNames": [
      "Windows",
      "mac",
      "macOS",
      "Linux"
    ],
    "mainClass": "ChromiumDriver"
  },
  "typedoc": {
    "entryPoint": "./build/lib/index.js"
  },
  "types": "./build/lib/index.d.ts",
  "devDependencies": {
    "@appium/eslint-config-appium": "8.0.5",
    "@appium/types": "0.17.0",
    "@appium/test-support": "^3.0.0",
    "@appium/tsconfig": "^0.x",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@tsconfig/node14": "14.1.2",
    "@types/bluebird": "3.5.42",
    "@types/lodash": "^4.14.191",
    "@types/mocha": "10.0.6",
    "@types/node": "20.12.2",
    "@types/teen_process": "^2.0.0",
    "asyncbox": "3.0.0",
    "chai": "^5.1.1",
    "chai-as-promised": "8.0.0",
    "conventional-changelog-conventionalcommits": "8.0.0",
    "eslint": "8.57.0",
    "prettier": "^3.0.0",
    "mocha": "10.6.0",
    "rimraf": "5.0.5",
    "semantic-release": "24.0.0",
    "sinon": "^18.0.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.3",
    "webdriverio": "8.39.1"
  }
}
